<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<!-- json包 -->
	<package name="answercountjson" extends="qhtaxijson" namespace="/statement/oncalltransaction/answercountjson">
	<!-- 抢答数据统计报表 -->
	<action name="*" method="{1}"
			class="com.careye.statement.oncalltransaction.action.AnswerCountAction">
			<result name="success" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
			<result name="error" type="json">
				<param name="includeProperties">success,result.*</param>
			</result>
	</action>
	</package>
	
</struts>
