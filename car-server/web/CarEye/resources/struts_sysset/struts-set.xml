<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
    "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<!-- jsonåŒ… -->
	<package name="setjson" extends="qhtaxijson" namespace="/setjson">
		<action name="*" method="{1}"
				class="com.careye.sysset.action.SetAction">
				<result name="success" type="json">
					<param name="includeProperties">success,result.*</param>
				</result>
				<result name="error" type="json">
					<param name="includeProperties">success,result.*</param>
				</result>
		</action>
		
		<action name="findPaymentParamById" method="findPaymentParamById"
				class="com.careye.sysset.action.SetAction">
				<result name="success" type="json">
					<param name="includeProperties">success,data.*</param>
				</result>
				<result name="error" type="json">
					<param name="includeProperties">success,data.*</param>
				</result>
		</action>
		
	</package>
	
</struts>
